<Application
    x:Class="Game_Life_WPF.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converter="clr-namespace:Game_Life_WPF.Resources.Converter"
    xmlns:local="clr-namespace:Game_Life_WPF"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <converter:ColorToBrushConverter x:Key="ColorToBrushConverter" />
        <converter:DrawingColorToMediaColorConverter x:Key="DrawingColorToMediaColorConverter" />
        <FontFamily x:Key="PressStart2P-Regular">
            /Resource/Font/PressStart2P-Regular.ttf #PressStart2PRegular
        </FontFamily>
        <FontFamily x:Key="Minecraft77">pack://application:,,,/Resource/Font/F77 Minecraft.ttf#Minecraft77</FontFamily>

    </Application.Resources>
</Application>
